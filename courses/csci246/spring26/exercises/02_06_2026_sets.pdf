\begin{problem}{3}
  Describe the set defined by the following set-builder notations.
  \vspace*{0.5em}

  \textbf{A}. $\{x \in \Z: x \equiv 1 (mod 3)\}$.
  \vspace*{0.5em}

  The set of integer numbers that when divided by 3 have a remainder of 1.
  \vspace*{1.5em}

  \textbf{B}. $\{x \in \Z: 1 < x \text{ and there is no } y \in Z \text{ s.t. } 1 < y < x \text{ and } y | x\}$
  \vspace*{0.5em}

  The set of prime numbers.
  \vspace*{1.5em}

  \textbf{C}. $\{x \in \Z: 0 \leq x\}$
  \vspace*{0.5em}

  The whole numbers
  \vspace*{1.5em}
\end{problem}


\begin{problem}{4}
  Let $A = \{a, b, c, d\}$.
  \vspace*{0.5em}

  \textbf{A}. List all subsets of $A$.
  \[
    \begin{array}{c}
      \emptyset\\
      \{a\} \qquad \{b\} \qquad \{c\} \qquad \{d\}\\
      \{a,b\} \qquad \{a,c\} \qquad \{a,d\} \qquad \{b,c\} \qquad \{b,d\} \qquad \{c,d\}\\
      \{a, b, c\} \qquad \{a, b, d\} \qquad \{a, c, d\} \qquad \{b, c, d\}\\
      \{a, b, c, d\}
    \end{array}
  \]

  \textbf{B}. Count the number of subsets by \emph{cardinality}.
  \vspace*{0.5em}

  Subsets of size 0: $1$

  Subsets of size 1: $4$

  Subsets of size 2: $6$

  Subsets of size 3: $4$

  Subsets of size 4: $1$
  \vspace*{0.5em}

  Total number of subsets: $16$
  \vspace*{0.5em}

  \textbf{C}. Write the powerset of $A$ (i.e., $2^A$ / $\mathcal{P}(A)$).
  \[
    \left\{
    \begin{array}{c}
      \emptyset,\\
      \{a\}, \qquad \{b\}, \qquad \{c\}, \qquad \{d\},\\
      \{a,b\}, \qquad \{a,c\}, \qquad \{a,d\}, \qquad \{b,c\}, \qquad \{b,d\}, \qquad \{c,d\},\\
      \{a, b, c\}, \qquad \{a, b, d\}, \qquad \{a, c, d\}, \qquad \{b, c, d\},\\
      \{a, b, c, d\}
    \end{array}
    \right\}
  \]

  \textbf{D}. What is the cardinality of $\mathcal{P}(A)$?
  \vspace*{0.25em}

  16
  \vspace*{1.25em}
\end{problem}

\begin{problem}{5}
  For each statement, decide whether its true for all sets or give a counter-example.

  \textbf{A}. if $A \subseteq B$, then $\mathcal{P}(A) \subseteq \mathcal{P}(B)$. \hfill true \hspace*{7cm}
  \vspace*{2em}

  \textbf{B}. if $A \subset B$, then $\mathcal{P}(A) \subseteq \mathcal{P}(B)$. \hfill true \hspace*{7cm}
  \vspace*{2em}

  \textbf{C}. $A \subseteq B$ and $B \subseteq A$ if and only if $A = B$. \hfill true \hspace*{7cm}
  \vspace*{2em}

  \textbf{D}. If $A \subset B$, then $|A| < |B|$. \hfill true \hspace*{7cm}
  \vspace*{2em}

  \textbf{E}. If $A \subseteq B$, then $|\mathcal{P}(A)| < |\mathcal{P}(B)|$. \hfill false. Consider $A = B = \emptyset$ \hspace*{3.7cm}
\end{problem}

